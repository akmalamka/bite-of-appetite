{
  "name": "biteofappetite.com",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prepare": "is-ci || simple-git-hooks",
    "devr": "pnpm --stream -r run dev",
    "dev": "nuxt dev",
    "lint:sanity-types": "eslint app/sanity/sanity.entity.ts --fix",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "dependencies": {
    "@nuxt/image": "^1.8.0",
    "@nuxtjs/sanity": "^1.12.1",
    "@nuxtjs/seo": "2.0.0-rc.19",
    "@portabletext/vue": "^1.0.11",
    "@vinicunca/perkakas": "^1.0.5",
    "@vueuse/core": "^11.0.3",
    "nuxt": "3.13.1",
    "radix-vue": "^1.9.5",
    "unocss": "^66.1.3",
    "vue": "^3.5.3",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@iconify/vue": "^4.1.2",
    "@unocss/nuxt": "^0.62.3",
    "@vinicunca/eslint-config": "^2.11.0",
    "@vinicunca/unocss-preset-vinicunca": "^0.2.2",
    "eslint": "^9.10.0",
    "firebase-tools": "^13.16.0",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.10",
    "postcss-nested": "^6.2.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx --no-install commitlint --edit \"$1\""
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm eslint --fix"
    ],
    "*.vue": [
      "pnpm eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}