{
  "functions": {
    "source": ".output/server",
    "runtime": "nodejs20"
  },
  "hosting": [
    {
      "target": "staging",
      "public": ".output/public",
      "cleanUrls": true,
      "rewrites": [
        {
          "source": "**",
          "function": "server_staging",
          "region": "asia-east1"
        }
      ]
    },
    {
      "target": "production",
      "public": ".output/public",
      "cleanUrls": true,
      "rewrites": [
        {
          "source": "**",
          "function": "server_production",
          "region": "asia-east1"
        }
      ]
    }
  ]
}